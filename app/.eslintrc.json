{
    "extends": "airbnb",
    "rules": {
        "import/no-extraneous-dependencies": ["error",  {"devDependencies": true}],
        "react/jsx-props-no-spreading": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
        "react/prop-types": 0,
        "no-shadow": "off",
        "camelcase": "off"
      }
}   